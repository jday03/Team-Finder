







<div class="album text-muted">
  <div class="container">
    <div class="row">
      <% @post.each do |p| %>
        <div class="card">
          <strong class="card-text"><%= p.title %></strong>
            <p class="card-text"><%= p.description %></p>
        </div>
      <% end %>
    </div>
    <div class="row">
      <div class="col-md-12">
        <%= paginate @parts %>
      </div>
    </div>
  </div>
</div>




